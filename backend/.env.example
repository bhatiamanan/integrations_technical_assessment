# Notion OAuth Credentials
# Get these from: https://www.notion.so/my-integrations
NOTION_CLIENT_ID=your_notion_client_id_here
NOTION_CLIENT_SECRET=your_notion_client_secret_here

# Airtable OAuth Credentials
# Get these from: https://airtable.com/oauth2/v1/applications
AIRTABLE_CLIENT_ID=your_airtable_client_id_here
AIRTABLE_CLIENT_SECRET=your_airtable_client_secret_here

# HubSpot OAuth Credentials
# Get these from: https://app.hubspot.com/l/app-marketplace/l/app-settings/apps/
HUBSPOT_CLIENT_ID=your_hubspot_client_id_here
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Server Configuration
SERVER_URL=http://localhost:8000
